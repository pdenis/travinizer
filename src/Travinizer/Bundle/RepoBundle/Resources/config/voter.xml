<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="travinizer.repo_voter.class">Travinizer\Bundle\RepoBundle\Voter\RepoVoter</parameter>
    </parameters>
    <services>
        <service id="travinizer.repo_voter" class="%travinizer.repo_voter.class%" public="true">
            <argument>%snide_travinizer.model.repo.class%</argument>
            <tag name="security.voter" />
        </service>
    </services>
</container>