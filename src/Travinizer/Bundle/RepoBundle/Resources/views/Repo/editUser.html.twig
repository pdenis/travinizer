{% extends 'SnideTravinizerBundle::base.html.twig' %}

{% block stylesheets %}

    {{ parent() }}
    {{ form_stylesheet(form) }}

{% endblock %}

{% block javascript %}


    {{ parent() }}

    {{ form_javascript(form) }}
{% endblock %}



{% block main %}
    <div class="row">
        <div id="content">
            <div class="title">
                <h2>Update a repository</h2>
            </div>

            <form action="{{ path('travinizer_repo_update_user', {'slug': repo.slug}) }}" method="post" {{ form_enctype(form) }}>

                {{ form_widget(form) }}
<div id="e6">   </div>
                <div class="form-actions">
                    <button class="btn-sub" type="submit" value="Submit">Submit</button>
                </div>
            </form>
        </div>
    </div>
{% endblock %}
